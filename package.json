{
    "name": "courses_list",
    "version": "1.0.0",
    "description": "Small project that allows you to chose your university courses, built using the MERN stack",
    "main": "server.js",
    "scripts": {
        "client-install": "npm install --prefix client",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "author": "Laith Grira",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.19.0",
        "concurrently": "^5.3.0",
        "express": "^4.17.1",
        "mongoose": "^5.10.3"
    },
    "devDependencies": {
        "nodemon": "^2.0.4"
    }
}